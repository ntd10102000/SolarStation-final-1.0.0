    <div class="modal fade" id="xem_anh_co_san">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" style="
                      text-align: center;
                      font-family: 'Times New Roman', Times, serif;
                    ">
                        XEM ẢNH CÓ SẴN
                    </h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/xemanhcosan_Admin" method="POST">
                        <div
                            style="margin: 10px 0 0 0; padding: 5px; max-height: 500px; overflow-x: auto; transition: all 0.2s ease;">
                            <div data-spy="scroll" data-target="#navbar-example2" data-offset="0">
                                <div class="row" style="padding: 5px; ">
                                    <label for="floatingInput" class="form-label"
                                        style="padding: 10px;width: 100%;">Chọn ảnh:
                                    </label>
                                    <%dsanh.forEach((e)=>{%>

                                        <div class="form-check" style="min-width: 100%; margin-left: 15px; ">
                                            <input class="form-check-input" name="anh" type="radio" value="<%= e.id%>"
                                                id="flexCheckDefault<%= e.name%>">
                                            <label class="form-check-label" for="flexCheckDefault">
                                                <%= e.name%>
                                            </label>
                                        </div>

                                        <%});%>


                                </div>
                            </div>
                        </div>

                </div>
                <div class="modal-footer justify-content-between">
                    <button type="button" class="btn btn-default" data-dismiss="modal">
                        Đóng
                    </button>
                    <button type="submit" class="btn btn-primary">
                        Xem ảnh
                    </button>
                </div>
                </form>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>
    <div class="modal fade" id="download_image">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" style="
                      text-align: center;
                      font-family: 'Times New Roman', Times, serif;
                    ">
                        TẢI ẢNH VỆ TINH
                    </h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/downloadAdmin" method="POST" id="taianhvetinh">
                        <label for="floatingInput" class="form-label" style="padding: 10px">Chọn
                            Loại Ảnh</label>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="loai_anh" id="exampleRadios1"
                                value="sentinel2" required />
                            <label class="form-check-label" for="flexRadioDefault1">
                                Sentinel
                            </label>
                        </div>
                        <div class="form-check">
                            <input class="form-check-input" type="radio" name="loai_anh" id="exampleRadios2"
                                value="landsat" required />
                            <label class="form-check-label" for="flexRadioDefault2">
                                Landsat
                            </label>
                        </div>

                        <label for="floatingInput" class="form-label" style="padding: 10px">Chọn
                            Ngày bắt đầu:
                        </label>
                        <input type="date" class="form-control" data-date-format="YYYY MM DD" name="startDate"
                            id="startDate" required />
                        <label for="floatingInput" class="form-label" style="padding: 10px">Chọn
                            Ngày kết thúc:
                        </label>
                        <input type="date" class="form-control" data-date-format="YYYY MM DD" name="endDate"
                            id="endDate" required />


                        <label for="floatingInput" class="form-label" style="padding: 10px;width: 100%;">
                            Chọn mã khách hàng:
                        </label>
                        <table id="example3" class="table">
                            <thead style="display: none;">
                                <tr>
                                    <th>
                                        </t>
                                    <th style="width: 30px;"></th>
                                </tr>
                            </thead>
                            <tbody>
                                <% dstrampin.rows.forEach(function(e){%>
                                    <tr>
                                        <td>
                                            <label class="form-check-label" for="flexCheckDefault<%= e.ma_kh%>">
                                                <%= e.ma_kh%>
                                            </label>
                                        </td>
                                        <td style="width: 60px;">
                                            |<input type="checkbox" class="form-check-input" name="check_ma_kh"
                                                value="<%= e.ma_kh%>" id="flexCheckDefault<%= e.ma_kh%>"
                                                style="margin-left: 20px;">
                                        </td>
                                    </tr>
                                    <%});%>
                            </tbody>
                        </table>

                </div>
                <div class="modal-footer justify-content-between">
                    <button type="button" class="btn btn-default" data-dismiss="modal">
                        Đóng
                    </button>
                    <button type="submit" class="btn btn-primary">
                        Tải ảnh
                    </button>
                </div>
                </form>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>
