<script>
    $(document).ready(function () {
        // $(".x").hide();
        $("#xoa").hide();
        $("#huy").hide();
        $(".x").click(function () {
            // $(".x").show();
            $("#xoa").show();
            $("#huy").show();
        });
        $("#themmoikhachhang").validate({
            rules: {
                cong_suat: {
                    number: true
                },
                lat: {
                    number: true
                },
                long: {
                    number: true
                },
            },
            messages: {
                cong_suat: {
                    number: "Công suất phải ở dạng số (ví dụ 900 hoặc 900.01)",
                },
                lat: {
                    number: "Nhập đúng định dạng vĩ độ (ví dụ: 20.2020)",
                },
                long: {
                    number: "Nhập đúng định dạng kinh độ (ví dụ: 100.01)",
                }
            }
        });
    });


</script>

